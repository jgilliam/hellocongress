<table cellpadding="0" cellspacing="0">
<% form_for :legislator, :url => { :controller => "legislators", :action => "search" } do |f| %>
<tr>
<td valign="middle"><%= f.text_field :name, :size => 25, :maxlength => 60, :autocomplete =>"off" %></td>
<td valign="middle" style="padding-left: 5px;">
	<%= f.submit "Search" %>
</td>
</tr>
</table>
<% end %>
<div id="search_links"><%= link_to_unless_current "Full list", legislators_url %></div>